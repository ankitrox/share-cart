{
    "name": "agade/woo-save-share-cart",
    "description": "Save and share cart for WooCommerce.",
    "type": "plugin",
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "wp-coding-standards/wpcs": "^3.0",
        "dealerdirect/phpcodesniffer-composer-installer": "^v0.7"
    },
    "scripts": {
        "php:lint": "vendor/bin/phpcs ."
    },
    "autoload": {
        "psr-4": {
            "Ankit\\WCSSC\\": "Includes"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Ankit Gade",
            "email": "contact@sharethingz.com"
        }
    ],
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
